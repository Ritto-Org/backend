{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"dynamicImport": true,
			"decorators": true
		},
		"target": "es2022",
		"baseUrl": "./",
		"loose": true,
		"externalHelpers": true,
		"keepClassNames": true,
		"paths": {
			"@/*": ["./src/*"],
			"@/app.controller": ["./src/app.controller.ts"],
			"@/app.service": ["./src/app.service.ts"],
			"@/app.module": ["./src/app.module.ts"]
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		}
	},
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true,
		"noInterop": false,
		"lazy": false
	},
	"minify": true
}
